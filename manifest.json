{
  "name": "ShellWarner",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Warn the sysadmins if they're vulnerable!",
  "homepage_url": "http://foo.bar",
  "icons": {
    "16": "icons/exclamation.png",
    "48": "icons/exclamation.png",
    "128": "icons/exclamation.png"
  },
  "permissions": [
    "webRequest",
    "*://*/*",
    "tabs",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  }
}